{
  "rules": {
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "max-len": [
      2,
      80,
      2
    ],
    "indent": [
      2,
      2
    ],
    "no-unused-vars": [
      2
    ]
  },
  "ecmaFeatures": {
    "modules": true,
    "arrowFunctions": true,
    "classes": true
  },
  "env": {
    "es6": true
  }
}
